{
	"name": "neo-fileheader",
	"displayName": "neo-fileheader",
	"description": "Conveniently and fully customizable fileheader extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"keywords": [
		"fileheader",
		"copyright",
		"文件头部注释",
		"许可证"
	],
	"activationEvents": [
		"onCommand:neo-fileheader.helloWorld",
		"workspaceContains:**/*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "neo-fileheader.fileheader",
				"title": "Add fileheader"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"ts-loader": "^9.3.0",
		"typescript": "^4.7.2",
		"webpack": "^5.72.1",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"dayjs": "^1.11.3",
		"lodash-es": "^4.17.21"
	}
}
